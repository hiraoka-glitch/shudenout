{
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        }
      ]
    }
  ],
  "rewrites": [],
  "redirects": [
    {
      "source": "/privacy",
      "destination": "/terms",
      "permanent": true
    },
    {
      "source": "/privacy-policy",
      "destination": "/terms", 
      "permanent": true
    }
  ]
}
